<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Books!</title>
</head>

<body>
  <%- include('title') %>


  <% books.forEach(function (book) {%>
    <h3>
      Book Title: <br><br>&emsp;<%= book.title %>
    </h3>
    <p>
      Book Author: <br><br>&emsp;<%= book.author %>
    </p>
  <% }) %>

</body>

</html>
